<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaolong.toothmanager.mapper.UserHospitalDetailMapper">

<select id="select" resultType="com.xiaolong.toothmanager.entity.userinfo.UserHospitalDetail">
    select * from
            user_hospital_detail
    where 1=1
    <if test="id != null">
        AND id=#{id}
    </if>
    <if test="username != null and username != ''">
        AND username=#{username}
    </if>
</select>
</mapper>
